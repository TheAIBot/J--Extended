package junit;

import junit.framework.TestCase;
import pass.Modulus;

public class ModulusTest extends TestCase {

    private Modulus modulus;

    protected void setUp() throws Exception {
        super.setUp();
        modulus = new Modulus();
    }

    protected void tearDown() throws Exception {
        super.tearDown();
    }

    public void testRemainder() {
        this.assertEquals(modulus.remainder(10,4), 2);
        this.assertEquals(modulus.remainder(8,4), 0);
        this.assertEquals(modulus.remainder(10,12), 10);
    }

}